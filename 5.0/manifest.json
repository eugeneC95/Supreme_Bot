{
  "manifest_version": 2,

  "name": "SupremeBot-Eugene_Chan",
  "description": "This is a Supreme bot which created by Eugene_Chan",
  "version": "1.0",

  "browser_action": {
     "default_icon": "icon.png",
     "default_title": "这是一个示例Chrome插件",
     "default_popup": "popup.html"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "notifications",
      "storage"
   ],
   "background":{
      "scripts": ["background.js"]
    },
}
