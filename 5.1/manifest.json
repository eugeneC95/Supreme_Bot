{
  "manifest_version": 2,

  "name": "SupremeBot-Eugene_Chan",
  "description": "This is a Supreme bot which created by Eugene_Chan",
  "version": "5.1",

  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "index.html"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "notifications",
      "storage",
      "<all_urls>"
  ],
  "background":{
    "scripts": ["background.js"],
    "persistent": false
  }
}
